{
    "name": "npm-package-boilerplate",
    "version": "1.0.0",
    "description": "Boilerplate NPM package.",
    "source": "src/index.js",
    "main": "dist/index.js",
    "scripts": {
        "build": "parcel build",
        "postinstall": "if [ -e ./scripts/postinstall.sh ]; then ./scripts/postinstall.sh; fi",
        "prepublishOnly": "npm run build",
        "test-unit": "jest --coverage=true ./test/unit --verbose --runInBand"
    },
    "files": [
        "dist/*"
    ],
    "author": "Luke Stanbery",
    "license": "MIT",
    "devDependencies": {
        "@jest/types": "^29.0.3",
        "jest": "^29.0.3",
        "node-notifier": "^10.0.1",
        "parcel": "^2.7.0"
    }
}
